mixin s-gal()
	// start s-gal
	+b.SECTION.s-gal.section.lazy-background#s-gal&attributes(attributes)
		.container
			+b.section-title.text-center
				h2 Некоторые из наших объектов
				
			+e.slider--js
				- for (var x = 0; x < 5; x++)
					+e.slide
						+e.img-wrap(href="img/@2x/rew.jpg" data-fancybox="gallery1")
							img.object-fit-js.res-i(src="#" data-lazy="img/@2x/rew.jpg", alt="")
	// end s-gal